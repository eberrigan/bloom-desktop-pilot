{
  "name": "bloom-desktop-pilot-2",
  "productName": "bloom-desktop-pilot-2",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "backend:start": "cd ./electric && docker compose up",
    "backend:stop": "cd ./electric && docker compose stop",
    "backend:restart": "cd ./electric && docker compose restart",
    "backend:up": "cd ./electric && docker compose up -d",
    "backend:down": "cd ./electric && docker compose down -v --remove-orphans",
    "client:generate": "npx electric-sql generate --service http://localhost:5133 --proxy postgresql://prisma:proxy_password@localhost:65432/postgres",
    "client:generate:debug": "npx electric-sql generate --service http://localhost:5133 --proxy postgresql://prisma:proxy_password@localhost:65432/postgres --debug",
    "client:watch": "npm run client:generate --watch",
    "db:reset": "supabase db reset --db-url postgresql://postgres:proxy_password@localhost:65432/postgres",
    "db:migrate": "npx supabase@beta db push --db-url postgresql://postgres:proxy_password@localhost:65432/postgres",
    "db:load-data": "node ./db/load-data.js"
  },
  "keywords": [],
  "author": {
    "name": "Dan Butler",
    "email": "daniel.jaffe.butler@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "autoprefixer": "^10.4.17",
    "css-loader": "^6.0.0",
    "electron": "28.2.2",
    "eslint": "^8.0.1",
    "eslint-plugin-import": "^2.25.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "node-loader": "^2.0.0",
    "postcss": "^8.4.35",
    "postcss-loader": "^8.1.0",
    "prisma": "^4.8.1",
    "style-loader": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "typescript": "~4.5.4"
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "@supabase/supabase-js": "^2.39.6",
    "@types/better-sqlite3": "^7.6.9",
    "@types/js-yaml": "^4.0.9",
    "@types/uuid": "^9.0.8",
    "better-sqlite3": "^9.4.1",
    "electric-sql": "^0.9.0",
    "electron-squirrel-startup": "^1.0.0",
    "js-yaml": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.0",
    "zod": "^3.22.4"
  }
}

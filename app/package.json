{
  "name": "bloom-desktop-pilot",
  "productName": "Bloom Desktop",
  "version": "1.0.0",
  "description": "My Electron application description",
  "main": ".webpack/main",
  "scripts": {
    "gen-types": "supabase gen types typescript --local > src/types/database.types.ts",
    "start": "electron-forge start",
    "ts-start": "ts-node ./forge.config.ts && electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "client:generate": "npx prisma generate",
    "client:watch": "npm run client:generate --watch",
    "db:generate": "npx prisma migrate dev --create-only",
    "db:deploy": "npx prisma migrate deploy"
  },
  "keywords": [],
